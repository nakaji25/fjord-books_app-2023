<div id="<%= dom_id report %>">
  <p>
    <strong>Title:</strong>
    <%= report.title %>
  </p>
  <p>
    <strong>Date:</strong>
    <%= report.created_at.strftime('%Y/%m/%d') %>
  </p>
  <p>
    <strong>Account:</strong>
    <%= report.user.name %>
  </p>
  <p>
    <strong>Content:</strong>
    <%= report.content %>
  </p>

</div>
